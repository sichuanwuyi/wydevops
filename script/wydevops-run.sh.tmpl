-A linux/amd64 \
-O linux/amd64 \
-B double \
-I /d/cachedImage \
-L java \
-S deploy \
-M local \
-T true \
-W "${_SCRIPT_ROOT_DIR}" \
-P "${_PROJECT_MAIN_MODULE_DIR}" \
-C "harbor,chartmuseum,192.168.1.214:80,admin,Harbor12345,80" \
-D "harbor,registry.docker.home,192.168.1.214:80,admin,Harbor12345,80"
